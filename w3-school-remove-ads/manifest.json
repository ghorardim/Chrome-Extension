{
    "manifest_version": 3,
    "name": "w3s block ads",
    "description": "Remove all ads from w3 school site.",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_icon": "stop-ads.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}